{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"outputs": ["./dist/**"],
			"dependsOn": ["^build"]
		},
		"test": {
			"outputs": ["coverage/**"],
			"dependsOn": ["build"],
			"outputMode": "full"
		},
		"lint": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"lint:ci": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	}
}
